            <button id='spelbord__veld{{@index}}{{@../index}}' class='spelbord__veld' onclick='Game.Reversi.showFishie({{@index}}, {{@../index}})'>
                {{~#ifequel this 1~}}
                    {{> fiche kleur="wit"}}
                {{~/ifequel~}}
                
                {{~#ifequel this 2~}}
                    {{> fiche kleur="zwart"}}
                {{~/ifequel~}}
            </button>
        {{~/each~}}
    {{~/each~}}
</div>